{
    "containers": {
      "frontend": {
        "image": "hokyun/frontend:latest",
        "ports": {
          "3000": "HTTP"
        }
      }
    },
    "publicEndpoint": {
      "containerName": "frontend",
      "containerPort": 3000,
      "healthCheck": {
        "healthyThreshold": 2,
        "unhealthyThreshold": 2,
        "timeoutSeconds": 5,
        "intervalSeconds": 10,
        "path": "/",
        "successCodes": "200-499"
      }
    }
  }
